{
  "name": "livechat/prestashop-module",
  "description": "LiveChat for Prestashop",
  "version": "1.1.0",
  "license": "AFL 3.0",
  "type": "prestashop-module",
  "authors": [
  {
    "name": "LiveChat, Inc.",
    "email": "integrations@livechatinc.com"
  }
  ],
  "require": {
    "php": "~5.6.0 | ~7.0.0 | ~7.1.0 | ~7.2.0 | ~7.3.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~5.0",
    "firebase/php-jwt": "^5.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.15",
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "LiveChatConnect\\Controllers\\": "controllers/",
      "LiveChatConnect\\Exceptions\\": "exceptions/",
      "LiveChatConnect\\Interfaces\\": "interfaces/",
      "LiveChatConnect\\Services\\": "services/",
      "LiveChatConnect\\Services\\Http\\": "services/http/"
    },
    "classmap": [
      "livechatconnect.php",
      "controllers/",
      "exceptions/",
      "interfaces/",
      "services/",
      "services/http/"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "prepend-autoloader": false
  }
}
